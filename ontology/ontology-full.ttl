    rdfs:domain ramp:Module ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( ramp:StudyArea ramp:StudyProgram ) ] ;
    owl:propertyChainAxiom ( ramp:isModuleOf ramp:isStudyAreaOf ) .

ramp:isStudyAreaOf a owl:ObjectProperty ;
