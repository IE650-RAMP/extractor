ramp:offeredIn a owl:DatatypeProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range [ a rdfs:Datatype ;
            owl:oneOf [ a rdf:List ;
                    rdf:first "FFS"^^xsd:string ;
                    rdf:rest [ a rdf:List ;
                            rdf:first "HWS"^^xsd:string ;
                            rdf:rest () ] ] ] .

ramp:recommended a owl:AnnotationProperty .

ramp:recommendedLiterature a owl:DatatypeProperty ;
    rdfs:label "Recommended Literature"@en .

ramp:recommendedSemester a owl:DatatypeProperty ;
    rdfs:label "Recommended Semester"@en ;
    rdfs:domain ramp:Module ;
    rdfs:range xsd:decimal .

ramp:workloadInPerson a owl:DatatypeProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range xsd:decimal .

ramp:workloadSelfStudy a owl:DatatypeProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range xsd:decimal .

ramp:hasModule a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( ramp:StudyArea ramp:StudyProgram ) ] ;
    rdfs:range ramp:Module ;
    owl:inverseOf ramp:isModuleOf ;
    owl:propertyChainAxiom ( ramp:hasStudyArea ramp:hasModule ) .

ramp:hasStudyArea a owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:inverseOf ramp:isStudyAreaOf .

ramp:isMandatoryPrerequisiteOf a owl:ObjectProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range ramp:Module ;
    rdfs:subPropertyOf ramp:isPrerequisiteOf .

ramp:isRecommendedPrerequisiteOf a owl:ObjectProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range ramp:Module ;
    rdfs:subPropertyOf ramp:isPrerequisiteOf .

