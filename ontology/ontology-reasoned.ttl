<http://anonymous> a owl:Ontology .

<http://ramp.uni-mannheim.de> a owl:Ontology .

ramp:additionalPrerequisite a owl:DatatypeProperty ;
    rdfs:domain ramp:Module .

ramp:ects a owl:DatatypeProperty ;
    rdfs:label "ECTS"^^xsd:string ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( ramp:Module ramp:StudyArea ) ] ;
    rdfs:range xsd:decimal .

ramp:examinationDistribution a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ;
            owl:intersectionOf ( ramp:Module [ a owl:Restriction ;
                        owl:hasSelf true ;
                        owl:onProperty ramp:hasAssesment ] ) ] ;
    rdfs:subPropertyOf owl:topDataProperty .

ramp:examinationDuration a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ;
            owl:intersectionOf ( ramp:Module [ a owl:Restriction ;
                        owl:hasSelf true ;
                        owl:onProperty ramp:hasAssesment ] ) ] .

ramp:hasDegree a owl:ObjectProperty ;
    rdfs:domain ramp:StudyProgram ;
    rdfs:range ramp:Degree .

ramp:hasLecturer a owl:ObjectProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range foaf:Person .

ramp:hasMandatoryPrerequisite a owl:ObjectProperty ;
    rdfs:subPropertyOf ramp:hasPrerequisite ;
    owl:inverseOf ramp:isMandatoryPrerequisiteOf .

ramp:hasPersonInCharge a owl:ObjectProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range foaf:Person .

ramp:hasRecommnededPrerequisite a owl:ObjectProperty ;
    rdfs:subPropertyOf ramp:hasPrerequisite ;
    owl:inverseOf ramp:isRecommendedPrerequisiteOf .

ramp:isChairOf a owl:ObjectProperty .

ramp:isFollowupOfModule a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:domain ramp:Module ;
    rdfs:range ramp:Module .

